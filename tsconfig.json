{
    "include": [
        "./src"
    ],
    "compilerOptions": {
        "declaration": true,
        "moduleResolution": "Node",
        "outDir": "./dist",
        "target": "ES2015",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "**/*.spec.ts"
    ]
}